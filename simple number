// BEGIN (write your solution here)
const isPrime=(num)=>{
  if (num<2){
    return false;
  }
  else if (num===2){
    return true;
}

 else if(num>2){
for(let count=2;count<=num;count++){
  if(num%count===0 && num!==count){
  
   
     return false;
     }
continue;
}


return true;


}
 }
    

// END


export default isPrime;


